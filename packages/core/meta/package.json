{
  "name": "vue-typewritter",
  "version": "0.0.4",
  "description": "This is a typewritter effect for vue3 and typescript",
  "keywords": [
    "typescript",
    "typewritter",
    "vue3",
    "vue"
  ],
  "homepage": "https://github.com/norvancc/typewritter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/norvancc/typewritter.git",
    "directory": "packages/core"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./*": "./*"
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "author": "norvancc",
  "license": "MIT"
}